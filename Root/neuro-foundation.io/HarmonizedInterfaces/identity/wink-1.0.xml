<?xml-stylesheet type="text/xsl" href="/Xsl/InterfaceToMarkdown.xslt"?>
<interface xmlns="urn:nfi:iot:hi:1.0" id="urn:nfi:iot:hi:identity:wink:1.0">
	<description>Allows clients to send a wink command to devices for real-world identification.</description>
	<commands>
		<simpleCommand name="Wink" use="Mandatory" description="Causes the device to wink in some way, so a user nearby can identify the device."/>"
	</commands>
</interface>